{
  "name": "episode-registry",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Behavioral Episode Registry - Mobile-first SSR application for tracking behavioral episodes",
  "author": "CGTAA Team",
  "license": "MIT",
  "scripts": {
    "dev": "node src/server/index.ts",
    "dev:ssr": "vite --ssr src/server/index.ts",
    "build": "vite build && vite build --ssr src/server/index.ts",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/server/index.ts --outDir dist/server",
    "build:ssr": "npm run build:client && npm run build:server",
    "preview": "node dist/server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write src/",
    "typecheck": "vue-tsc -b"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "^4.3.3",
    "@aws-sdk/client-cognito-identity-provider": "^3.893.0",
    "@aws-sdk/client-dynamodb": "^3.893.0",
    "@aws-sdk/lib-dynamodb": "^3.893.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/uuid": "^11.0.0",
    "aws-amplify": "^6.15.6",
    "aws-jwt-verify": "^5.1.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jspdf": "^3.0.3",
    "pdf-lib": "^1.17.1",
    "pinia": "^3.0.3",
    "sirv": "^3.0.2",
    "uuid": "^13.0.0",
    "vite-plugin-ssr": "^0.4.142",
    "vue": "^3.5.21",
    "vue-router": "4",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "vite": "^7.1.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  }
}
